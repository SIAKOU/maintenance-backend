{
  "name": "maintenance-backend",
  "version": "1.0.0",
  "description": "Backend pour syst√®me de gestion de maintenance",
  "main": "server.js",
  "scripts": {
    "start": "node -r ./scripts/setupEnv.js server.js",
    "dev": "nodemon -r ./scripts/setupEnv.js server.js",
    "setup-env": "node ./scripts/setupEnv.js",
    "test": "jest",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "reset-db": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npm run seed"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chalk": "^5.4.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fs": "^0.0.1-security",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.0",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0",
    "sharp": "^0.34.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.1",
    "node-mocks-http": "^1.14.1"
  }
}
